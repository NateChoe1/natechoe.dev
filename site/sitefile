declare TCP 80

timeout 10000 80

set port 80

define library /site/library.so

set host www\\.natechoe\\.dev
set type text/html
read / /site/site/wrong.html
throw .* 404

set host .*
set type text/css
linked /autogen/colors.css
read .*\\.css /site/site/

set type text/html
read / /site/site/index.html
read .*\\.(html|ncdg) /site/site/

set type text/plain
read .*\\.txt /site/site/
read /info/public.key /site/site/info/public.key

set type image/png
read .*\\.png /site/site/

set type text/javascript
read .*\\.js /site/site/
# Ew javascript.

set type image/vnd.microsoft.icon
linked /favicon\\.ico /site/site/favicon.ico
